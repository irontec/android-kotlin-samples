<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.irontec.examples.kotlintest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" >
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OkHttpAsyncTaskActivity"/>
        <activity android:name=".AnkoAsyncTaskActivity"/>
        <activity android:name=".ListviewBaseAdapterDataObjActivity"/>
        <activity android:name=".HttpUrlConnectionAsyncActivity"/>
        <activity android:name=".AnkoSimpleUiActivity"/>
        <activity android:name=".GsonDeserializerOkhttpAsyncActivity"/>

    </application>

</manifest>